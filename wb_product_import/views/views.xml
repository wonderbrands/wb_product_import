<odoo>
  <data>
    <record  id="wb_product_import_form_inherit" model="ir.ui.view">
      <field name="name">wb_product_import_inherit</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_form_view"/>
      <field name="arch" type="xml">
           <xpath expr="//page[4]" position="before" >
            <page name="Import Product" string="Importaci贸n">
              <group string="Importaci贸n">
                <field name="tariff_percentage" />
                <field name="official_mexican_standards"/>
                <field name="l10n_mx_edi_tariff_fraction_id" />
              </group>
            </page>
          </xpath>
      </field>
    </record>

    <record  id="wb_purchase_import_form_inherit" model="ir.ui.view">
      <field name="name">wb_purchase_import_inherit</field>
      <field name="model">purchase.order</field>
      <field name="inherit_id" ref="purchase.purchase_order_form"/>
      <field name="arch" type="xml">
           <xpath expr="//page[1]" position="after" >
            <page name="Import Product" string="Importaci贸n">
              <group string="Importaci贸n">
               <field name="general_import_tax"/>
               <field name="value_added_tax"/>
               <field name="customs_processing_right"/>
               <field name="prevalidation"/>
               <field name="vat_prevalidation"/>
               <field name="customs_charges"/>
               <field name="storages"/>
               <field name="delays"/>
               <field name="land_freight"/>
               <field name="sea_freight"/>
               <field name="salary_customs_agents"/>
              </group>
            </page>
          </xpath>
      </field>
    </record>

        <record model="ir.ui.view" id="noms_tree_view">
            <field name="name">NOMS Tree</field>
            <field name="model">l10n.mx.edi.noms.noms</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="noms_form_view">
            <field name="name">NOMS Form</field>
            <field name="model">l10n.mx.edi.noms.noms</field>
            <field name="arch" type="xml">
                <form>
                        <group>
                            <field name="code" required="1"/>
                            <field name="name" required="1"/>
                        </group>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="noms_action">
            <field name="name">NOMS</field>
            <field name="res_model">l10n.mx.edi.noms.noms</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="noms_tree_view"/>
        </record>
  </data>
</odoo>
